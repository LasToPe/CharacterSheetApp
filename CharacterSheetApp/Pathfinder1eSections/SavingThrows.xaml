<UserControl x:Class="CharacterSheetApp.Pathfinder1eSections.SavingThrows"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CharacterSheetApp.Pathfinder1eSections"
             xmlns:components="clr-namespace:CharacterSheetApp.Pathfinder1eSections.Components"
             mc:Ignorable="d" d:DesignWidth="800">
    <StackPanel>
        <components:SavingThrow SaveName="Fortitude" SaveAbility="Constitution" />
        <components:SavingThrow SaveName="Reflex" SaveAbility="Dexterity" />
        <components:SavingThrow SaveName="Will" SaveAbility="Wisdom" />
    </StackPanel>
</UserControl>
