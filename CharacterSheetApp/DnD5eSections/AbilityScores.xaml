<UserControl x:Class="CharacterSheetApp.DnD5eSections.AbilityScores"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CharacterSheetApp.DnD5eSections"
             xmlns:components="clr-namespace:CharacterSheetApp.DnD5eSections.Components"
             mc:Ignorable="d" d:DesignWidth="75">
    <StackPanel>
        <components:AbilityScore AbilityName="STRENGTH" />
        <components:AbilityScore AbilityName="DEXTERITY" />
        <components:AbilityScore AbilityName="CONSTITUTION" />
        <components:AbilityScore AbilityName="INTELLIGENCE" />
        <components:AbilityScore AbilityName="WISDOM" />
        <components:AbilityScore AbilityName="CHARISMA" />
    </StackPanel>
</UserControl>
